{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
	"http://www.google.com/*"
  ],

  "content_scripts": [
	{
	    "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
        ],
        "js": ["runscript.js"],
		"run_at": "document_end",
		"all_frames":true		
	}
  ]
}